public class NumberInWord {
    public static void printNumberInWord(int wholeNum){

            if(wholeNum == 0) System.out.println("ZERO");
            else if(wholeNum == 1) System.out.println("ONE");
            else if(wholeNum == 2)System.out.println("TWO");
            else if(wholeNum == 3)System.out.println("THREE");
            else if(wholeNum == 4)System.out.println("FOUR");
            else if(wholeNum == 5)System.out.println("FIVE");
            else if(wholeNum == 6)System.out.println("SIX");
            else if(wholeNum == 7)System.out.println("SEVEN");
            else if(wholeNum == 8)System.out.println("EIGHT");
            else if(wholeNum == 9)System.out.println("NINE");
            else System.out.println("Other");
    }
}